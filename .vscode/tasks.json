{
  "tasks": [
    {
      "type": "shell",
      "label": "CMake build",
      "command": "cmake --build build",
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ],
  "version": "2.0.0"
}